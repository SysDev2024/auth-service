{
  "family": "TaskAuth",
  "containerDefinitions": [
    {
      "name": "Auth",
      "image": "381491874375.dkr.ecr.eu-north-1.amazonaws.com/services:latest",
      "cpu": 0,
      "portMappings": [
        {
          "name": "auth-8080-port",
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "systemControls": []
    }
  ],
  "taskRoleArn": "arn:aws:iam::381491874375:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::381491874375:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["EC2"],
  "cpu": "1024",
  "memory": "768",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
